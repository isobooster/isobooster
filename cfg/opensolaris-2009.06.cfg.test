ISOFILE=osol-0906-x86.iso
BASEURL=http://dlc.sun.com/osol/opensolaris/2009/06/$ISOFILE

#install
prepareiso $ISOFILE $BASEURL
#copyiso opensolaris-2009.06 $ISOFILE

#remove
purgefile /iso/$ISOFILE

#menu
menuentry "OpenSolaris 2009.06" {
  savedefault
  loopback loop /iso/$ISOFILE
  chainloader (loop)+1
}
